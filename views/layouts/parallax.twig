 	{% if layouts.image_or_video == 'image' %}
       	<div class="parallax-window" data-parallax="scroll" data-image-src="{{TimberImage(layouts.para_image).src }}"></div>
	{% elseif layouts.image_or_video == 'video' %}
		<div class="video-container">
		  <video autoplay poster="" class="video-parallax" loop muted>
		    <source src="{{ layouts.video_link }}" type="video/webm">
		  </video>
		</div>
	{% endif %}